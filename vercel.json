{
  "github": { "silent": true },
  "redirects": [
    {
      "source": "/post/(.*)",
      "destination": "/posts/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/fonts/.*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=3600, stale-while-revalidate"
        }
      ]
    },
    {
      "source": "/images/.*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=3600, stale-while-revalidate"
        }
      ]
    }
  ]
}
